<table class="ui very basic large table">
  <thead>
    <tr>
      <th class="six wide">
        <%= link_to t('.edit'), edit_admin_ticket_path(@ticket), class: 'ui mini blue right floated button', remote: true %>
      </th>
      <th class="ten wide"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td class="right aligned"><%= Ticket.human_attribute_name(:match_value) %></td>
      <td><%= @ticket.match_value %></td>
    </tr>
    <tr>
      <td class="right aligned"><%= Ticket.human_attribute_name(:serial_start) %></td>
      <td><%= @ticket.serial_start %></td>
    </tr>
    <tr>
      <td class="right aligned"><%= Ticket.human_attribute_name(:start_at) %></td>
      <td><%= @ticket.start_at %></td>
    </tr>
    <tr>
      <td class="right aligned"><%= Ticket.human_attribute_name(:finish_at) %></td>
      <td><%= @ticket.finish_at %></td>
    </tr>
    <tr>
      <td class="right aligned"><%= Ticket.human_attribute_name(:valid_response) %></td>
      <td><%= @ticket.valid_response %></td>
    </tr>
    <tr>
      <td class="right aligned"><%= Ticket.human_attribute_name(:invalid_response) %></td>
      <td><%= @ticket.invalid_response %></td>
    </tr>
  </tbody>
</table>
