<%= form_with model: [:admin, @ticket] do |f| %>
  <%= render 'shared/error_messages', target: @ticket %>
  <%= f.text_field :serial_start %>
  <%= f.text_field :valid_response %>
  <%= f.text_field :invalid_response %>
  <%= f.time_select :start_at, default: '0:00'.to_time %>
  <%= f.time_select :finish_at, default: '23:59'.to_time %>
  <%= f.submit %>
<% end %>
